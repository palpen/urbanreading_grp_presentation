\begin_deeper
\begin_layout Itemize
Firms cluster to reduce shipping costs
\end_layout

\begin_layout Itemize
1987 Input-Output account as measure of extent to which industries buy and
 sell from one another 
\end_layout

\begin_layout Itemize
\begin_inset Formula $Input_{i\rightarrow j}=0.39$
\end_inset

: Leather Tanning and Finishing input from Meat Products
\end_layout

\begin_layout Itemize
\begin_inset Formula $Output_{i\rightarrow j}=0.82$
\end_inset

: Public building and Related Furniture sales to Motor Vehicles and Equipment
\end_layout

\begin_layout Itemize
Most values for an industry pair is zero 
\end_layout

\begin_layout Itemize
\begin_inset Formula $Input_{ij}=max\{Input_{i\rightarrow j},Input_{i\leftarrow j}\}$
\end_inset

, 
\begin_inset Formula $Output_{ij}=max\{Output_{i\rightarrow j},Output_{i\leftarrow j}\}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $InputOutput_{ij}=max\{Input_{ij},Output_{ij}\}$
\end_inset


\end_layout

