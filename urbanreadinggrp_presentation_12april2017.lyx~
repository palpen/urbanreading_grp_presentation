\begin_layout Subsection
Duranton-Overman Index (ReStud, 2005)
\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Duranton-Overman Index !!!
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\[
\hat{K}_{ij}^{Emp}(d)=\frac{1}{h\sum_{r=1}^{n_{i}}\sum_{s=1}^{n_{j}}e(r)e(s)}\sum_{r=1}^{n_{i}}\sum_{s=1}^{n_{j}}e(r)e(s)f\left(\frac{d-d_{r,s}}{h}\right)
\]

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $e(r)$
\end_inset

 : employment in establishment 
\begin_inset Formula $r$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $d_{r,s}$
\end_inset

 : distance between establishments 
\begin_inset Formula $r$
\end_inset

 and 
\begin_inset Formula $s$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $h$
\end_inset

 : bandwidth
\end_layout

\begin_layout Itemize
\begin_inset Formula $n_{i}$
\end_inset

 : number of firms in industry 
\begin_inset Formula $i$
\end_inset

 
\end_layout

\begin_layout Itemize
\begin_inset Formula $f(.)$
\end_inset

 : Gaussian kernel (bell curve)
\end_layout

\end_deeper
\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Duranton-Overman Index !!!
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\[
\hat{K}_{ij}^{Emp}(d)=\frac{1}{h\sum_{r=1}^{n_{i}}\sum_{s=1}^{n_{j}}e(r)e(s)}\sum_{r=1}^{n_{i}}\sum_{s=1}^{n_{j}}e(r)e(s)f\left(\frac{d-d_{r,s}}{h}\right)
\]

\end_inset


\end_layout

\begin_layout Itemize
Explicitly accounts for distance between establishment (missing in EG index)
\end_layout

\begin_layout Itemize
Index of localization:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\gamma_{ij}(d)\equiv max(\hat{K}_{ij}^{Emp}(d)-\bar{K}_{ij}^{Emp}(d),0)
\]

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $\bar{K}_{ij}^{Emp}(d)$
\end_inset

 is the upper 5% confidence interval
\end_layout

\begin_layout Itemize
Estimate for each industry pair 
\begin_inset Formula $ij$
\end_inset

 and each distance 
\begin_inset Formula $d\in\{100km,250km,500km,1000km\}$
\end_inset


