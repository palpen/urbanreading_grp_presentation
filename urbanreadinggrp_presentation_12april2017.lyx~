\begin_layout Section
Measuring Extent of Coagglomeration
\end_layout

\begin_layout Subsection
Ellison-Glaeser Index (JPE, 1997)
\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Ellison-Glaeser Index
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\[
\gamma_{ij}^{c}=\frac{\sum_{m=1}^{M}(s_{mi}-x_{m})(s_{mj}-x_{m})}{1-\sum_{m=1}^{M}x_{m}^{2}}
\]

\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $s_{mi}=\text{share of indistry i's employment}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula $x_{m}=\text{mean employment share in region \ensuremath{m} across all manufacturing industries}$
\end_inset


\end_layout

\begin_layout Itemize
Two industry version of index developed in Ellison and Glaeser, JPE 1997
\end_layout

\begin_layout Itemize
Derived from a model where local industry-specific spillovers, natural advantage
s, and random chance all contribute to geographic concentration
\end_layout

\begin_layout Itemize
Level: state, PMSA, county
\end_layout

\end_deeper
\begin_layout Frame

\end_layout

\begin_layout Standard
\begin_inset Separator parbreak
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Ellison-Glaeser Index
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\[
\gamma_{ij}^{c}=\frac{\sum_{m=1}^{M}(s_{mi}-x_{m})(s_{mj}-x_{m})}{1-\sum_{m=1}^{M}x_{m}^{2}}
\]

\end_inset


\end_layout

